{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "comma-dangle": "off",
        "object-shorthand": "off",
        "object-curly-newline": "off",
        "no-unused-vars": "off",
        "no-underscore-dangle" : "off",
        "no-param-reassign": "off",
        "no-undef": "off"
    }
}
